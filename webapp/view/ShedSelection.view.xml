<mvc:View xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="ShedStatusReport.controller.ShedSelection">
	<App>
		<pages>
			<Page title="{i18n>title}">
				<content>
					<TileContainer id="sheds" allowAdd="false" editable="false" tiles="{data>/sheds}">
						<StandardTile icon="sap-icon://factory" number="{data>SHEDCODE}" info="Estatus" numberUnit="{i18n>titleshed}" title="{i18n>longitude}: {data>STALLLONGITUDE} - {i18n>width}: {data>STALLWIDTH}" press="handlePress"/>
					</TileContainer>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>


